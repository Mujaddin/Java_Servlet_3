package serveltPackage;

public class Student {

	private Integer id;
	private String name;

public Student(String name,int i) {
	// TODO Auto-generated constructor stub
	this.id=i;
	this.name=name;
}


public int getId() {
	return id;
}

public void setId(int id) {
	this.id = id;
}

public String getName() {
	return name;
}

public void setName(String name) {
	this.name = name;
}


}
